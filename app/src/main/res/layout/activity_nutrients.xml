<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple_700"
    tools:context=".Nutrients">


    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/purple_700"
        tools:context=".Nutrients">
    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="421dp"
        android:layout_height="234dp"
        app:srcCompat="@drawable/clams"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="15dp" />

        <TextView
            android:layout_width="156dp"
            android:layout_column="2"
            android:text="Clams\n"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:textStyle="bold" />

        <TableRow android:layout_width="match_parent">

            <TextView
                android:layout_width="156dp"
                android:layout_column="1"
                android:text="Nutrients"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
            android:layout_width="218dp"
            android:gravity="right"
            android:text="Amount"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:textStyle="bold"
            tools:ignore="RtlCompat" />
    </TableRow>
    <TableRow android:layout_width="match_parent">

        <TextView
            android:layout_width="192dp"
            android:layout_column="1"
            android:text="Calories"
            android:textAlignment="center"
            android:textColor="@color/white" />

        <TextView
            android:layout_width="200dp"
            android:gravity="right"
            android:text="178 kcal"
            android:textAlignment="center"
            android:textColor="@color/white"
            tools:ignore="RtlCompat" />
    </TableRow>
    <TableRow android:layout_width="match_parent">

        <TextView
            android:layout_width="192dp"
            android:layout_column="1"
            android:text="Carbohydrate"
            android:textAlignment="center"
            android:textColor="@color/white" />

        <TextView
            android:layout_width="200dp"
            android:gravity="right"
            android:text="2.6g"
            android:textAlignment="center"
            android:textColor="@color/white"
            tools:ignore="RtlCompat" />
    </TableRow>
    <TableRow android:layout_width="match_parent">

        <TextView
            android:layout_width="192dp"
            android:layout_column="1"
            android:text="Protein"
            android:textAlignment="center"
            android:textColor="@color/white" />

        <TextView
            android:layout_width="200dp"
            android:gravity="right"
            android:text="12.8g"
            android:textAlignment="center"
            android:textColor="@color/white"
            tools:ignore="RtlCompat" />
    </TableRow>

    <TableRow android:layout_width="match_parent">

        <TextView
            android:layout_width="192dp"
            android:layout_column="1"
            android:text="Vitamin B12"
            android:textAlignment="center"
            android:textColor="@color/white" />

        <TextView
            android:layout_width="200dp"
            android:gravity="right"
            android:text="49.4 mcg"
            android:textAlignment="center"
            android:textColor="@color/white"
            tools:ignore="RtlCompat" />
    </TableRow>

    <TableRow android:layout_width="match_parent">

        <TextView
            android:layout_width="192dp"
            android:layout_column="1"
            android:text="Vitamin C"
            android:textAlignment="center"
            android:textColor="@color/white" />

        <TextView
        android:layout_width="200dp"
        android:gravity="right"
        android:text="13 mg"
        android:textAlignment="center"
        android:textColor="@color/white"
        tools:ignore="RtlCompat" />

    </TableRow>

    <TableRow android:layout_width="match_parent">

        <TextView
            android:layout_width="192dp"
            android:layout_column="1"
            android:text="Iron"
            android:textAlignment="center"
            android:textColor="@color/white" />

        <TextView
            android:layout_width="200dp"
            android:gravity="right"
            android:text="14mg"
            android:textAlignment="center"
            android:textColor="@color/white"
            tools:ignore="RtlCompat" />
    </TableRow>

    <TableRow android:layout_width="match_parent">

        <TextView
            android:layout_width="192dp"
            android:layout_column="1"
            android:text="Selenium"
            android:textAlignment="center"
            android:textColor="@color/white" />

        <TextView
            android:layout_width="200dp"
            android:gravity="right"
            android:text="24.3 mg"
            android:textAlignment="center"
            android:textColor="@color/white"
            tools:ignore="RtlCompat" />
    </TableRow>

    <TableRow android:layout_width="match_parent">

        <TextView
            android:layout_width="192dp"
            android:layout_column="1"
            android:text="Calcium"
            android:textAlignment="center"
            android:textColor="@color/white" />

        <TextView
            android:layout_width="200dp"
            android:gravity="right"
            android:text="46mg"
            android:textAlignment="center"
            android:textColor="@color/white"
            tools:ignore="RtlCompat" />
    </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
             >



            <TextView android:text="   " />

            <Button
                android:id="@+id/backfromn1"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_marginBottom="316dp"

                android:fontFamily="@font/extralight"
                android:text="Back to home"
                android:textColor="@color/white"
                android:textSize="19sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.084"
                app:layout_constraintStart_toStartOf="parent" />

            <Button
                android:id="@+id/nextonnutr"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginBottom="316dp"

                android:fontFamily="@font/extralight"
                android:text="Next"
                android:textColor="@color/white"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.084"
                app:layout_constraintStart_toStartOf="parent" />

        </TableRow>
    </TableLayout>


</RelativeLayout>